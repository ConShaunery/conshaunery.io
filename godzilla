// JavaScripting by OVEN Digital - NOT TO BE USED WITHOUT PERMISSION
//-------------------------start preload - image names---------------
initpics("preview","action","adventure","cardandboard","sports", "heat")        
function initpics(allpics) {
        imgName = new Array()
        for (i = 0;i < initpics.arguments.length; i++) {
                imgName[i] = initpics.arguments[i]
        }
        }
        picarray = new Array()
        for(j = 0; j < imgName.length; j++) {
      picarray[j] = new Image(29,29)
      picarray[j].src = "/gifs/icon_" + imgName[j] + "_hi.gif"        }
//-------------------------end pre load------------------------------
               
       function img_act(imgName) {
               document [imgName].src = "/gifs/icon_" + imgName + "_hi.gif";
			   if (imgName == "preview") _text = "Preview Channel";
			   else if (imgName == "action") _text = "Action and Strategy";
               else if (imgName == "adventure") _text = "Adventure";
			   else if (imgName =="cardandboard") _text = "Card and Board";
               else if (imgName =="sports") _text = "Sports";
               else if (imgName =="heat") _text = "HEAT.NET";
               window.status = _text;
               }

       function img_inact(imgName) { 
               document [imgName].src = "/gifs/icon_" + imgName + ".gif";
               defaultStatus=" ";
               }
</SCRIPT>

    <A HREF="/preview/" onMouseover="img_act('preview'); return true" onMouseout="img_inact('preview')">
